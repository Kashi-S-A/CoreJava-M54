package Package1;

public class P1 {

	static int a = 10;
	
	private static int b = 20;
	
	protected static int c = 30;
	
	public static void main(String[] args) {
		System.out.println(a);
		System.out.println(P1.a);
		System.out.println(b);
		System.out.println(c);
	}
	
	public static int getA() {
		return a;
	}
}
